---
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /tags/{{ tag }}/
layout: single
eleventyComputed:
  title: Tagged “{{ tag }}”
---

<ul class="post-list">
{% for post in collections[ tag ] | reverse %}
  <li class="post-listing mb0 mt0"><header class="post-listing__header"><span class="post__date">{{ post.date | toFormattedDate }}</span></header> <a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ul>